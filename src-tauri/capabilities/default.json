{
	"$schema": "../gen/schemas/desktop-schema.json",
	"identifier": "default",
	"description": "Capability for the main window",
	"windows": ["main"],
	"permissions": [
		"core:default",
		"core:app:allow-default-window-icon",
		"core:window:allow-set-focus",
		"core:window:allow-show",
		"opener:default",
		"store:default",
		"dialog:default",
		"updater:default",
		"process:default",
		"os:default",
		"os:allow-hostname"
	]
}
